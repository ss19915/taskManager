    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.9",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
